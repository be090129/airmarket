<%= form_for([@listing, @order]) do |f| %>

  <div class="field">
    <%= f.label :start_date %><br>
    <%= f.date_select :start_date %>
  </div>
  <div class="field">
    <%= f.label :end_date %><br>
    <%= f.date_select :end_date %>
  </div>
  <div class="field">
    <%= f.label :order_price %><br>
    <%= f.number_field :order_price %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
